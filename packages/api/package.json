{
  "name": "@repo/api-types",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./utility": {
      "default": "./dist/utility/index.js",
      "types": "./dist/utility/index.d.ts"
    },
    "./utility/*": {
      "default": "./dist/utility/*.js",
      "types": "./dist/utility/*.d.ts"
    },
    "./route": {
      "default": "./dist/path/index.js",
      "types": "./dist/path/index.d.ts"
    },
    "./route/auth": {
      "default": "./dist/path/auth/index.js",
      "types": "./dist/path/auth/index.d.ts"
    },
    "./data": {
      "default": "./dist/data/index.js",
      "types": "./dist/data/index.d.ts"
    },
    "./data/*": {
      "default": "./dist/data/*.js",
      "types": "./dist/data/*.d.ts"
    }
  },
  "scripts": {
    "build": "tsc"
  },
  "devDependencies": {
    "@repo/typescript-config": "*",
    "@turbo/gen": "^1.10.12",
    "@types/node": "^20.5.2",
    "eslint": "^8.53.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "axios": "^1.6.3"
  }
}